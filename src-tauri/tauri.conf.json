{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Onir",
  "version": "0.1.0",
  "identifier": "com.moxiang.onir",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Onir",
        "width": 900,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/android-icon-192.png",
      "icons/android-icon-512.png"
    ],
    "android": {
      "minSdkVersion": 24
    }
  },
  "plugins": {}
}
